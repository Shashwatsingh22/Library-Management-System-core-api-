{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Response for login",
  "example": {},
  "type": "object",
  "properties": {
    "id": {
      "type": "string"
    },
    "user": {
      "name": {
        "type": "string"
      },
      "email": {
        "type": "string"
      },
      "userName": {
        "type": "string"
      },
      "password": {
        "type": "string"
      },
      "role": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer"
          }
        }
      },
      "designation": {
        "type": "string"
      },
      "college": {
        "type": "string"
      }
    },
    "ipAddress": {
      "type": "string"
    },
    "deviceInfo": {
      "type": "object",
      "properties": {
        "osVersion": {
          "type": "string"
        },
        "browser": {
          "type": "string"
        },
        "browserVersion": {
          "type": "string"
        },
        "deviceName": {
          "type": "string"
        }
      }
    },
    "userAgent": {
      "type": "string"
    }
  }
}